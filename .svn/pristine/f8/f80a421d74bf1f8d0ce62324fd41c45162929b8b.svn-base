<ion-header>
  <ion-navbar>
    <button ion-button menuToggle>
      <ion-icon name="menu"></ion-icon>
    </button>
    <ion-title>GSM预警监控</ion-title>
    <ion-buttons end>
      <button ion-button icon-only (click)="doLogout()">
        <ion-icon ios="ios-log-out" md="md-log-out"></ion-icon>
      </button>
    </ion-buttons>

  </ion-navbar>
</ion-header>


<ion-content>
  <ion-item [navPush]="monitorPage">
    <ion-thumbnail item-start>
      <img src="assets/img/indicator.jpg"/>
    </ion-thumbnail>
    <h2>指标监控</h2>
  </ion-item>
  <ion-item [navPush]="processPage">
    <ion-thumbnail item-start>
      <img src="assets/img/list.jpg"/>
    </ion-thumbnail>
    <h2>工单处理</h2>
  </ion-item>
  <ion-item [navPush]="extractPage">
    <ion-thumbnail item-start>
      <img src="assets/img/download.jpg"/>
    </ion-thumbnail>
    <h2>工单提取</h2>
  </ion-item>
  <ion-item [navPush]="batchProcessPage">
    <ion-thumbnail item-start>
      <img src="assets/img/batch.jpg"/>
    </ion-thumbnail>
    <h2>工单批量处理</h2>
  </ion-item>
  <ion-item [navPush]="statisticsPage">
    <ion-thumbnail item-start>
      <img src="assets/img/statistic.jpg"/>
    </ion-thumbnail>
    <h2>工单统计</h2>
  </ion-item>
</ion-content>


