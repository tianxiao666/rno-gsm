<ion-header>
  <ion-navbar>
    <ion-title>工单列表</ion-title>
    <ion-buttons end (click)="toIndex()">
      <button ion-button icon-only>
        <ion-icon name="md-home">
        </ion-icon>
      </button>
    </ion-buttons>
  </ion-navbar>
</ion-header>
<ion-content>
  <ion-list ion-item *ngFor="let data of datas" (click)="getDetailById(data[0])">
      <h5 style="white-space: pre-wrap">{{data[0]}}</h5>
      <p><span>{{data[3]}}</span>
        <span>{{data[4]}}</span>
      </p>
    <p>{{data[2]}}</p>
    <ion-note item-end>
      <ion-icon name="arrow-forward" style="float:right"></ion-icon><br><br>
        <span style="float: right">{{data[1]}}</span>
    </ion-note>
  </ion-list>

  <ion-infinite-scroll (ionInfinite)="doInfinite($event)">
    <ion-infinite-scroll-content></ion-infinite-scroll-content>
  </ion-infinite-scroll>
</ion-content>
